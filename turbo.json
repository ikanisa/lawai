{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["pnpm-lock.yaml"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": [],
      "dependsOn": ["^typecheck"]
    },
    "test": {
      "outputs": ["coverage/**", "test-results/**"],
      "dependsOn": ["^test"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", ".next/**", "out/**"]
    },
    "check:migrations": {
      "cache": false
    }
  }
}

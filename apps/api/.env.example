# Core runtime
NODE_ENV=development
PORT=3333
LOG_LEVEL=info

# OpenAI configuration
OPENAI_API_KEY=sk-<paste-openai-api-key>
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_VECTOR_STORE_AUTHORITIES_ID=vs_<authorities-id>
AGENT_MODEL=gpt-4o
EMBEDDING_MODEL=text-embedding-3-large
AGENT_STUB_MODE=auto

# Optional OpenAI overrides
OPENAI_ORG_ID=<optional>
OPENAI_PROJECT_ID=<optional>
OPENAI_WEBHOOK_SECRET=<optional>
OPENAI_REQUEST_TAGS=service=api
OPENAI_REQUEST_TAGS_API=service=api,component=backend
OPENAI_REQUEST_TAGS_CHATKIT=service=api,component=chatkit
OPENAI_REQUEST_TAGS_SUMMARIES=service=api,component=summaries
SUMMARISER_MODEL=<optional>
MAX_SUMMARY_CHARS=8000

# Supabase access
SUPABASE_URL=https://<project-ref>.supabase.co
SUPABASE_SERVICE_ROLE_KEY=<paste-supabase-service-role-key>

# Optional integrations
OPENAI_CHATKIT_PROJECT=<optional>
OPENAI_CHATKIT_SECRET=<optional>
OPENAI_CHATKIT_BASE_URL=<optional>
OPENAI_CHATKIT_MODEL=<optional>
JURIS_ALLOWLIST_JSON=["fr"]
WA_TOKEN=<optional>
WA_PHONE_NUMBER_ID=<optional>
WA_TEMPLATE_OTP_NAME=<optional>
WA_TEMPLATE_LOCALE=fr_FR
WA_PROVIDER=meta
C2PA_SIGNING_PRIVATE_KEY=<optional>
C2PA_SIGNING_KEY_ID=<optional>
POLICY_VERSION=<optional>

# Core runtime
NODE_ENV=development

# Supabase access
SUPABASE_URL=https://<project-ref>.supabase.co
SUPABASE_SERVICE_ROLE_KEY=<paste-supabase-service-role-key>
SUPABASE_DB_URL=postgresql://postgres:postgres@localhost:5432/postgres
SUPABASE_PROJECT_REF=<project-ref>

# OpenAI configuration
OPENAI_API_KEY=sk-<paste-openai-api-key>
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_VECTOR_STORE_AUTHORITIES_ID=vs_<authorities-id>
OPENAI_REQUEST_TAGS=service=ops
OPENAI_REQUEST_TAGS_OPS=service=ops,component=worker

# Safety toggles
OPS_CHECK_DRY_RUN=0
VECTOR_STORE_DRY_RUN=0

# Optional management APIs
SUPABASE_MANAGEMENT_API_URL=https://api.supabase.com/v1/projects
SUPABASE_ACCESS_TOKEN=<optional-management-token>

# Default identifiers
OPS_ORG_ID=00000000-0000-0000-0000-000000000000
EVAL_ORG_ID=00000000-0000-0000-0000-000000000000
EVAL_USER_ID=00000000-0000-0000-0000-000000000000
API_BASE_URL=http://localhost:3333/api

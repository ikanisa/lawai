{
  "name": "@avocat-ai/api-sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "generate": "openapi --input ../../apps/api/openapi.json --output src/generated --client fetch --useUnionTypes true",
    "prebuild": "pnpm run generate",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "verify": "pnpm run generate && git diff --exit-code -- src/generated"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.57.0",
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "^5.4.5"
  }
}
